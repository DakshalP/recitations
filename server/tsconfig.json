{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@interfaces/*": ["../common/interfaces/*"],
      "@models/*": ["../common/models/*"],
      "@controllers/*": ["library/controllers/*"],
      "@enums/*": ["../common/enums/*"],
      "@routes/*": ["library/routes/*"],
      "@helpers/*": ["library/helpers/*"],
    },
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },
}
