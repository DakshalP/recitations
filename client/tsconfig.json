{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@environment": ["./src/environments/environment"],
      "@services/*": ["./src/app/services/*"],
      "@models/*": ["./src/app/models/*"],
      "@components/*": ["./src/app/modules/components/*"],
      "@pages/*": ["./src/app/modules/pages/*"],
      "@interfaces/*": ["../common/interfaces/*"],
      "@enums/*": ["../common/enums/*"],
    }
  }
}
